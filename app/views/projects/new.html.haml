%section.content-header
  %h1 Projects
%hr
%section.content
  .box
    .box-header
      %h3 Create Projects
    .box-body.table-responsive
    = form_for @project do |f|
      .row
        .col-md-12
          = f.label :name
          = f.text_field :name
      .row
        .col-md-12
          = f.label :description
          = f.text_field :description
      .row
        .col-md-12
          = f.submit :onclick => "validateProjForm();"
